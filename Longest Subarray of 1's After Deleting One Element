class Solution:
    def longestSubarray(self, nums: List[int]) -> int:
        l=0
        max_count=0
        zero=-1
        for r in range(len(nums)):
            if nums[r]==1:
                max_count=max(max_count,r-l+1)
            else:
                if zero==-1:
                    max_countcount = max(max_count,r-l+1)
                    zero=r
                else:
                    l=zero+1
                    zero = r
        return max_count-1
