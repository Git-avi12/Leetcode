class Solution(object):
    def dayOfYear(self, date):
        dno = 0
        daycount = [31,28,31,30,31,30,31,31,30,31,30,31]
        for i in range(0,int(date[5:7])-1):
            dno+=daycount[i]
        dno+=int(date[8:])

        if int(date[0:4])% 4 == 0 and int(date[0:4])% 100 != 0 or int(date[0:4])% 400 == 0 :
            if int(date[5:7]) > 2:
                dno+=1
        return dno
        
