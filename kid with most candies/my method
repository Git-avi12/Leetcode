class Solution(object):
    def kidsWithCandies(self, candies, extraCandies):
        maxval=max(candies)
        boolist=[]
        for i in candies:
            if i + extraCandies < maxval:
                boolist.append(False)
            else:
                boolist.append(True)
        return boolist
        """
        :type candies: List[int]
        :type extraCandies: int
        :rtype: List[bool]
        """
        
