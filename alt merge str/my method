class Solution(object):
    def mergeAlternately(self, word1, word2):
        str_final=""
        w1=w2=0
        while((w1+w2)<(len(word1+word2))):
            if(w1<len(word1)):
                str_final+=word1[w1]
                w1+=1
            if(w2<len(word2)):
                str_final+=word2[w2]
                w2+=1
        return str_final
        """
        :type word1: str
        :type word2: str
        :rtype: str
        """
        
