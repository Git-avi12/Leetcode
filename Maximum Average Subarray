class Solution(object):
    def findMaxAverage(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: float
        """
        Window_sum=sum(nums[:k])
        Final_output=Window_sum/float(k)
        for r in range(k,len(nums)):
            Window_sum = Window_sum - nums[r-k] + nums[r]
            Final_output = max(Window_sum/float(k),Final_output)
        return Final_output




        '''while r<len(nums):
            Window_sum = Window_sum - nums[l] + nums[r]
            Final_output = max(Final_output,Window_sum/float(k))
