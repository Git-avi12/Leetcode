    def removeDuplicates(self, nums):
        k = 1
        count = 1
        for i in range(1,len(nums)):
            if nums[i] != nums[i-1]:
                count = 1
                nums[k] = nums[i]
                k+=1
            else:
                if count < 2:
                    nums[k] = nums[i]
                    k+=1
                    count+=1
        return k
