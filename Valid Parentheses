class Solution:
    def isValid(self, s: str) -> bool:
        stack = []
        val = {"}":"{",")":"(","]":"["}
        for i in s:
            if i in val.values():
                stack.append(i)
            else:
                if not stack or stack[-1] != val[i]:
                    return False
                stack.pop()
        return not stack


