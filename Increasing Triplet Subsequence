class Solution(object):
    def increasingTriplet(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        num1=float("inf")
        num2=float("inf")
        for i in nums:
            if i<=num1:
                num1=i
            elif i<=num2:
                num2=i
            else:
                return True
        return False
            
