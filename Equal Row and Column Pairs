from collections import Counter
class Solution:
    def equalPairs(self, grid: List[List[int]]) -> int:
        n = len(grid)
        row_count = Counter(tuple(row) for row in grid)
        print(row_count)

        cnt = 0
        for i in range(n):
            col = tuple(grid[j][i] for j in range(n))
            cnt += row_count[col]
        return cnt
